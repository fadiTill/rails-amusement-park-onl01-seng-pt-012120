<%= form_for user do |f| %>

    <label> Name:</label><br>
  <%= f.text_field :name %><br>

    <label> password:</label><br>
  <%= f.text_field :password %><br>

  <label>Nausea:</label><br>
  <%= f.number_field:nausea %><br>

